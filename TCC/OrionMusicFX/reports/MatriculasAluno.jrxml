	<parameter name="codAluno" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT `Matricula`.cod,
	`Aluno`.nome,
	`Disciplina`.nome,
	`Turma`.`descricaoTurma`,
	`Matricula`.valoraula,
	`Matricula`.`valorFormatado`,
	`Aluno`.cod,
	`Matricula`.`codAluno`,
	`Matricula`.`codTurma`,
	`Matricula`.`arquivoMorto`,
	`Disciplina`.cod,
	`Turma`.`codDisciplina`,
	`Turma`.`codTurma`
FROM `Matricula`,
	`Aluno`,
	`Disciplina`,
	`Turma`
WHERE 
	 `Aluno`.cod = `Matricula`.`codAluno` 
	 AND `Disciplina`.cod = `Turma`.`codDisciplina` 
	 AND `Turma`.`codTurma` = `Matricula`.`codTurma` 
	 AND `Matricula`.`arquivoMorto` = 'NULL' 
	 AND `Aluno`.cod = $P{codAluno} ]]>
	</queryString>
